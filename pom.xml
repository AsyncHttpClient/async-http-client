        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <netty.version>4.1.85.Final</netty.version>
        <slf4j.version>2.0.3</slf4j.version>
        <reactive-streams.version>1.0.3</reactive-streams.version>
